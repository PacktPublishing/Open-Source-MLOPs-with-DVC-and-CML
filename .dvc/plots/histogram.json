{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
      "values": "<DVC_METRIC_DATA>"
  },
  "mark": "bar",
  "title": "<DVC_METRIC_TITLE>",
  "width": 300,
  "height": 300,
  "encoding": {
    "x": {
      "bin": {"extent": [0, 7000], "step": 500},
      "field": "<DVC_METRIC_X>",
      "title": "<DVC_METRIC_X_LABEL>"
    },
    "y": {
      "aggregate": "count"
    }
  }
}
